[include]
   path = ~/.gitconfig.local
[color]
   status = auto
   diff = auto
   branch = auto
   interactive = auto
   grep = auto
[push]
   default = tracking
[core]
   excludesfile = ~/.gitignore
   editor = vim
[alias]
   st = status -sb
   co = checkout
   ci = commit
   p = push -u
   b = branch
   up = fetch --verbose --prune
   d = diff
   dc = diff --cached
   ds = diff --staged
   pick = cherry-pick
   changed = whatchanged
   l = log --pretty=format:\"%ci %C(yellow)%h%Creset [%cn] %Cgreen%s %C(cyan)%d%Creset\" -10
   lg = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
