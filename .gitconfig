[include]
   path = ~/.gitconfig.local
[color]
   status = auto
   diff = auto
   branch = auto
   interactive = auto
   grep = auto
[push]
   default = tracking
[pull]
   rebase = true
[merge]
   ff = false
[core]
   excludesfile = ~/.gitignore
   editor = vim
   pager = less -r
   autocrlf = input
[alias]
   st = status -sb
   su = submodule update
   co = checkout
   ci = commit
   p = push -u
   b = branch
   up = fetch --verbose --prune origin
   d = diff --find-renames
   dc = diff --cached --find-renames
   ds = diff --staged --find-renames
   pick = cherry-pick
   changed = whatchanged
   l = log --pretty=format:\"%ci %C(yellow)%h%Creset [%cn] %Cgreen%s %C(cyan)%d%Creset\" -10
   lg = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
